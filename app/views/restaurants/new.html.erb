<h1>Add new restaurant</h1>
<%= form_with model: @restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :rating%>
  <%= f.text_field :rating %>

  <%= f.submit%>
<% end %>

<%= link_to "< Back", restaurants_path %>
